.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  rapidjson_config = debug
  rapidjson_extra = 
endif
ifeq ($(config),release)
  rapidjson_config = release
  rapidjson_extra = 
endif

PROJECTS := rapidjson

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

rapidjson:
ifneq (,$(rapidjson_config))
	@echo "==== Building rapidjson ($(rapidjson_config)) ===="
	@${MAKE} --no-print-directory -C . -f rapidjson.make config=$(rapidjson_config) $(rapidjson_extra)
endif

clean:
	@${MAKE} --no-print-directory -C . -f rapidjson.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   rapidjson"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
